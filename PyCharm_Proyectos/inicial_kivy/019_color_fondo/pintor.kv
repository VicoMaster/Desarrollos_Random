#:import C kivy.utils.get_color_from_hex

<FVerde@FloatLayout>:
    size_hint: .3, .3
    canvas.before:
        Color:
            #rgba: 1, 1, 1, 1
            rgba: C("#22FFAA")
        Rectangle:
            pos: self.pos
            size: self.size


FloatLayout:
    FVerde:
        pos_hint: {"x": .4, "y": .4}
